    "import math \n",
    "\n",
    "nivelQuant = int(input(\"Informe o nível de quantização:\"))\n",
    "if(nivelQuant <= 0):\n",
    "\tprint(\"Inválido\")\n",
    "elif(nivelQuant > 0):\n",
    "  bits = int (math.log2(nivelQuant))\n",
    "  print(bits)\n",
    "\n",
    "  for i in range(2**bits):\n",
    "\t  print(f' {i:0{bits}b} ')"
   ]
